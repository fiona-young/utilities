{
  "name": "matters/utilities",
  "description": "Php utilities",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "proprietary",
  "authors": [
    {
      "name": "Fiona Matters",
      "email": "fiona@matters.com"
    }
  ],
  "require": {
    "symfony/dependency-injection": "v2.5.4",
    "symfony/yaml": "v2.7.24",
    "symfony/config": "v2.7.24",
    "symfony/http-foundation": "v2.6.6"
  },
  "require-dev": {
    "phpunit/phpunit": "4.7.*",
    "phpunit/phpunit-mock-objects": "2.3.1"
  },
  "autoload": {
    "psr-4": {"Matters\\Utilities\\": "Src/"}
  },
  "autoload-dev": {
    "psr-4": {"Matters\\Utilities\\Test\\": "Test/"}
  }
}
